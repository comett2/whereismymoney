{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": ".dkr.ecr..amazonaws.com/wimm-backend:latest",
      "essential": true,
      "portMappings": [{ "hostPort": 8080, "containerPort": 8080 }]
    },
    {
      "name": "frontend",
      "image": ".dkr.ecr..amazonaws.com/wimm-frontend:latest",
      "essential": true,
      "portMappings": [{ "hostPort": 80, "containerPort": 80 }]
    }
  ]
}
